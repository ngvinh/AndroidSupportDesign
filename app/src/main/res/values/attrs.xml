<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseTheme">
        <attr name="actionBarIconColor" format="color" />
        <attr name="actionBarInsetStart" format="dimension" />
        <attr name="spinnerBarInsetStart" format="dimension" />
        <attr name="popupItemBackground" format="reference" />
        <attr name="photoItemForeground" format="reference" />
        <attr name="photoItemForegroundBorderless" format="reference" />
    </declare-styleable>

    <declare-styleable name="ScrimInsetsView">
        <attr name="insetForeground01" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="CustomView">
        <attr name="font" format="string" />
    </declare-styleable>

    <declare-styleable name="SlidingLayer">
        <attr name="shadowDrawableSliding" format="boolean" />
        <attr name="shadowWidthSliding" format="dimension" />
        <attr name="closeOnTapEnabled" format="boolean" />
        <attr name="openOnTapEnabled" format="boolean" />
        <attr name="offsetWidth" format="dimension" />
        <attr name="stickTo" format="enum">
            <enum name="auto" value="0" />
            <enum name="right" value="-1" />
            <enum name="left" value="-2" />
            <enum name="middle" value="-3" />
            <enum name="top" value="-4" />
            <enum name="bottom" value="-5" />
        </attr>
    </declare-styleable>
</resources>